{
  "name": "vue-ics",
  "description": "Vue.js plugin for generating ICalendar (.ics) files.",
  "version": "0.1.4",
  "author": "Stanislav Mihaylov <ceo@pepper.llc>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/evildvl/vue-ics.git"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "demo": "vue-cli-service demo",
    "docs": "npm run docs:serve",
    "docs:build": "vue-cli-service docs --mode build",
    "docs:serve": "vue-cli-service docs --mode serve",
    "prepublish": "vue-cli-service lint && vue-cli-service build",
    "start": "vue-cli-service serve"
  },
  "main": "dist/vue-ics.common.js",
  "module": "dist/vue-ics.esm.js",
  "unpkg": "dist/vue-ics.umd.min.js",
  "files": [
    "dist/vue-ics.common.js",
    "dist/vue-ics.umd.min.js",
    "dist/vue-ics.umd.js",
    "dist/vue-ics.esm.js",
    "src"
  ],
  "dependencies": {
    "core-js": "^3.0.0",
    "file-saver": "^2.0.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^5.0.4",
    "@vue/cli-plugin-eslint": "^5.0.4",
    "@vue/cli-service": "^5.0.4",
    "babel-eslint": "10.1.0",
    "cz-conventional-changelog": "^3.0.1",
    "eslint": "7.7.0",
    "eslint-plugin-vue": "6.2.2",
    "vue": "2.6.11",
    "vue-cli-plugin-p11n": "^0.0.2",
    "vue-template-compiler": "2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "jsdelivr": "dist/vue-ics.umd.min.js",
  "sideeffects": false,
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
